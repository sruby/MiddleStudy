<?xml version="1.0" encoding="utf-8"?>
<manifest package="io.github.sruby.middlestudy"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".fragment.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".fragment.FragmentAty">
        </activity>
        <activity
            android:name=".fragment.SlideAty"
            android:label="@string/title_activity_slide_aty"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>